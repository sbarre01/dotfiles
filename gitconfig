[core]
	safecrlf = warn
	deltaBaseCacheLimit = 1G
	excludesfile = ~/.gitignore
	quotepath = false
	editor = vim
[push]
	default = simple
[branch "master"]
	rebase = true
	autosetuprebase = true
[branch]
	autosetuprebase = always
[status]
	displaycommentprefix = false
[rebase]
	stat = true
[hub]
	protocol = https
[color]
	ui = auto
	# diff = auto
	# status = auto
	# branch = auto
[color "branch"]
	#current = yellow reverse
	#local = yellow
	#remote = green
[color "diff"]
	#meta = yellow bold
	#frag = magenta bold
	#old = red bold
	#new = green bold
[color "status"]
	#added = yellow
	#changed = green
	#untracked = cyan
[apply]
	whitespace = nowarn
[rerere]
	enabled = true
[diff]
	tool = bc3
	algorithm = patience
[difftool]
	prompt = false
[merge]
	tool = bc3
[mergetool]
	prompt = false
	keepBackup = false
	bc3 = trustExitCode
